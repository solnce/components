{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "es2016",
    "jsx": "react-jsx",
    "jsxImportSource": "@emotion/react",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "paths": {
      "@atoms/*": ["./components/atoms/*"],
      "@styles": ["./components/const/styles.ts"],
    },
  }
}
