{
  "name": "@solnce/components",
  "version": "0.0.4",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./dist/index.js"
  },
  "repository": "git@github.com:solnce/components.git",
  "engines": {
    "node": "^16.0.0"
  },
  "author": "solnce <solnce.jpn@gmail.com>",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "dependencies": {
    "@emotion/babel-preset-css-prop": "^11.2.0",
    "@emotion/react": "^11.9.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^4.7.4"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@emotion/eslint-plugin": "^11.7.0",
    "@secretlint/secretlint-rule-preset-recommend": "^5.2.3",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/builder-webpack4": "^6.5.9",
    "@storybook/manager-webpack4": "^6.5.9",
    "@storybook/react": "^6.5.9",
    "@storybook/testing-library": "^0.0.13",
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@types/jest": "^28.1.6",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "babel-loader": "^8.2.5",
    "chromatic": "^6.7.0",
    "eslint": "^7.32.0 || ^8.2.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "secretlint": "^5.2.3",
    "storybook": "^6.5.9",
    "stylelint": "^14.9.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-prettier": "^2.0.0",
    "ts-jest": "^28.0.5",
    "tsc-alias": "^1.6.11",
    "tsconfig-paths-webpack-plugin": "^3.5.2"
  },
  "resolutions": {
    "got": "^11.8.5",
    "trim-newlines": "^3.0.1",
    "glob-parent": "^5.1.2",
    "trim": "^0.0.3"
  },
  "scripts": {
    "build": "tsc --project ./tsconfig.build.json && tsc-alias -p ./tsconfig.build.json",
    "check-types": "tsc --project ./tsconfig.json --noEmit",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "stylelint": "stylelint --ignore-path .gitignore \"components/**/*.+(css|ts|tsx)\"",
    "lint-fix": "eslint --cache --fix ./src/components && prettier --write ./src/components",
    "lint": "eslint --cache ./src/components",
    "secretlint": "secretlint '**/*'",
    "test": "jest"
  },
  "eslintIgnore": [
    "/.eslintrc.js"
  ],
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!(xxxx.*?\\.js$))"
    ],
    "testRegex": "(/test/.*|\\.(test|spec))\\.(ts|tsx)$",
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx"
    ],
    "moduleNameMapper": {
      "^@atoms/(.*)": "<rootDir>/src/components/atoms/$1",
      "^@molecules/(.*)": "<rootDir>/src/components/molecules/$1",
      "^@styles$": "<rootDir>/src/components/const/styles.ts"
    },
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.ts"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{ts,tsx,js,jsx}": [
      "prettier --write './src/components'",
      "eslint --cache --fix './src/components'"
    ]
  }
}
